---
layout: null
permalink: /background-photos/json/
---
{

	"background-photos": [

		{% for post in site.categories.background-photos %}
		{% if post != site.categories.background-photos[0] %}
		,
		{% endif %}
		{
			"title": "{{ post.title }}"
			,"url": "{{ post.url }}"
			,"photo-url": "{{ post.photo-url }}"
			,"photo-description": "{{ post.photo-description }}"
		}
		{% endfor %}

	]
	
}